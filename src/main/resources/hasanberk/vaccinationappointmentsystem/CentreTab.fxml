<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="750.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hasanberk.vaccinationappointmentsystem.Controllers.CentreTabController">
   <children>
      <Button layoutX="825.0" layoutY="180.0" mnemonicParsing="false" onAction="#addCentreClicked" prefWidth="89.0" text="Add Centre" />
      <ListView fx:id="listView" prefHeight="750.0" prefWidth="800.0" />
      <TextField fx:id="nameTextField" layoutX="825.0" layoutY="25.0" promptText="Name" />
      <TextField fx:id="addressTextField" layoutX="825.0" layoutY="55.0" promptText="Address" />
      <TextField fx:id="eirCodeTextField" layoutX="825.0" layoutY="85.0" promptText="EirCode" />
      <Spinner fx:id="parkingSpotsSpinner" editable="true" layoutX="825.0" layoutY="143.0" />
      <Label layoutX="822.0" layoutY="125.0" text="Parking Spaces" />
      <Button layoutX="825.0" layoutY="214.0" mnemonicParsing="false" onAction="#deleteCentreClicked" text="Delete Centre" />
   </children>
</AnchorPane>
